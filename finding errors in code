#DECLARE noCookies : INTEGER
print ("Enter numvber of cookies to buy")
noCookies = int(input())
Cost = noCookies * 0.40
print ("Total cost is", round(Cost,2))

#DECLARE distance : INTEGER
#DECLARE speed : REAL
#DECLARE time : INTEGER
print ("Input distance in miles")
distance = int(input())
print ("Input time in hours")
time = int(input())
speed = distance/time
print ("The speed is", round(speed,3))

